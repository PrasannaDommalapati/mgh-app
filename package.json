{
  "name": "hotels-app",
  "version": "1.0.0",
  "scripts": {
    "start": "typings install && webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "test-local": "npm prune && npm install && npm run build && mkdir -p build/unit && npm test",
    "e2e-local": "node config/e2e-ts-local.js",
    "build-production": "rimraf dist && typings install && webpack --config config/webpack.production.js --progress --profile --bail",
    "build-sandbox": "rimraf dist && typings install && webpack --config config/webpack.sandbox.js --progress --profile --bail",
    "build-staging": "rimraf dist && typings install && webpack --config config/webpack.staging.js --progress --profile --bail",
    "build-docker": "docker build -tags latest headforwardsspd/node-cd . && docker push headforwardsspd/node-cd"
  },
